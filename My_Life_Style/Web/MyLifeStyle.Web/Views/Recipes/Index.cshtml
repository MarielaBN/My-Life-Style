@using MyLifeStyle.Web.ViewModels.Recipes
@model IEnumerable<AllRecipesViewModel>

@{
    this.ViewData["Title"] = "Recipes";
}

<h1>@this.ViewData["Title"]</h1>

@foreach (var recipes in Model)
{
    <div>@recipes.PublicationTitle /@recipes.DishType /@recipes.DietType, @recipes.PublicationCreatedOn (@recipes.CommentsCount) commets</div>
}